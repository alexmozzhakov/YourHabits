<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomFont">
        <attr name="font" format="string"/>
    </declare-styleable>

    <declare-styleable name="timeline_style">
        <attr name="marker" format="color|reference"/>
        <attr name="line" format="color|reference"/>
        <attr name="marker_size" format="dimension"/>
        <attr name="line_size" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="SwipeStringSelector">
        <attr name="swipe_indicatorSize" format="dimension" />
        <attr name="swipe_indicatorMargin" format="dimension" />
        <attr name="swipe_indicatorActiveColor" format="color" />
        <attr name="swipe_indicatorInActiveColor" format="color" />
        <attr name="swipe_leftButtonResource" format="reference" />
        <attr name="swipe_rightButtonResource" format="reference" />
        <attr name="swipe_customFontPath" format="string" />
        <attr name="swipe_titleTextAppearance" format="reference" />
        <attr name="swipe_descriptionTextAppearance" format="reference" />

        <attr name="swipe_descriptionGravity" format="enum">
            <enum name="left" value="0"/>
            <enum name="center" value="1" />
            <enum name="right" value="2"/>
        </attr>
    </declare-styleable>

    <attr name="highContrastTextColor" format="reference"/>
    <attr name="mediumContrastTextColor" format="reference"/>
    <attr name="lowContrastTextColor" format="reference"/>
</resources>